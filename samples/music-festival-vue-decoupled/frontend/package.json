{
  "name": "music-festival-decoupled",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "content-definitions:push": "content-definitions push manifest.json -s http://localhost:8081 --authority http://localhost:8081 --client-id cli --client-secret cli --allowed-upgrades major",
    "content-definitions:pull": "content-definitions pull manifest.json -s http://localhost:8081 --authority http://localhost:8081 --client-id cli --client-secret cli"
  },
  "dependencies": {
    "@episerver/content-delivery": "file:../../../src/@episerver/content-delivery",
    "core-js": "^3.8.3",
    "lodash": "^4.17.20",
    "oidc-client-ts": "^2.0.6",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@episerver/content-definitions": "file:../../../src/@episerver/content-definitions",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "import-glob-loader": "^1.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "linebreak-style": "off",
      "no-console": "off",
      "no-shadow": "off",
      "max-len": "off",
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
