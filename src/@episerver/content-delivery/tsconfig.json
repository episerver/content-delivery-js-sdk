{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "NodeNext",
    "esModuleInterop": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "lib/",
  },
}
